{
    "UseInMemoryDatabase": true,
    "ConnectionStrings": {
        "ApplicationDb": "<your-connection-string>",
        "IdentityDb": "<your-connection-string>"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Jwt": {
        "Secret": "<your-secret>",
        "Issuer": "<your-issuer>",
        "Audience": "<your-audience>",
        "Expiration": "28d"
    },
    "CorsOrigins": null,
    "AllowedHosts": "*"
}
